<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_editor_absoluteY="500dp"
            android:layout_height="47dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="122dp"
            android:id="@+id/Login" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            motion:layout_editor_absoluteY="401dp"
            android:layout_height="47dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="122dp"
            android:id="@+id/Login" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="122dp"
            android:layout_height="47dp"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="394dp"
            android:id="@+id/Login"
            motion:transitionEasing="cubic(0.11,0.7,0.95,0.2)" />
        <Constraint
            android:id="@+id/Login"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="122dp"
            android:layout_height="47dp"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="394dp"
            motion:transitionEasing="cubic(0.19,0.78,0.8,0.8)" />
        <Constraint
            android:id="@+id/Login"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="122dp"
            android:layout_height="47dp"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="394dp"
            motion:transitionEasing="cubic(0.19,0.78,0.98,0.31)" />
    </ConstraintSet>
</MotionScene>